<% unless ext.functions.empty? %>
module Functions
  <% ext.functions.each do |_, fun| %>
  def <%= fun.signature %> end
  <% end %>

  Parameters = {
    <% max_len = ext.max_function_name_length %>
    <% ext.functions.each do |_, fun| %>
    <%= fun.mapped(max_len) %>,
    <% end %>
  }.freeze
end
<% end %>